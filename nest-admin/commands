1.docker-compose up
2.nest generate module user
3.nest generate controller user
4. npm install --save @nestjs/typeorm typeorm mysql     
5.nest generate module auth   
6.nest generate controller auth   

7.npm i bcrypt
8.npm i -D @types/bcrypt

9.npm install class-validator --save
10.npm install --save @nestjs/jwt passport-jwt
11.npm install --save-dev  @types/passport-jwt

12.nest generate interceptor auth/auth
13.nest generate guard auth/auth
14.nest generate module role   